Print the Pattern 
for n = 5
//
5432*
543*1
54*21
5*321
*4321
//


import java.util.Scanner;
public class Solution{
   public static void main(String[] args){
     Scanner s = new Scanner(System.in);
     int n = s.nextInt();
     for(int i=1; i<=n; i++){
        for(int j=n; j>=1; j--){
          if(i == j){
             System.out.print("*");
          }else{
             System.out.print(j);
          }
        }
       System.out.println();
      }

        
