//
*****
 ****
  ***
   **
    *
//

import java.util.Scanner;
public class Solution{
   public static void main(String[] args){
     Scanner s = new Scanneer(System.in);
     int n = s.nextInt();
     for(int i=1; i<=n; i++){
       for(int j=1; j<=n; j++){
         if(j >= i){
            System.out.print("*");
         }else{
            System.out.print(" ");
         }
       }
      System.out.println();
      }
   }
}
