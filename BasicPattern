import java.util.Scanner;
public class Solution{
  public static void main(Stsring[] args){
   Scanner s = new Scanner(System.in);
   int n = s.nextInt();
   for(int i=1; i<+n; i++){
     for(int j=1; j<=n; j++){
      System.out.print("#");
    }
   System.out.println();
   }
}
}
